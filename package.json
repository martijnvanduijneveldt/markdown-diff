  "dependencies": {
    "diff": "^5.2.0",
    "marked": "^12.0.2"
  },
  "devDependencies": {
    "@types/diff": "^5.2.0",
    "@typescript-eslint/parser": "^7.7.1",
    "chai": "^5.1.0",
